
# ML Platform .gitignore

# Cached data and models
cache/
**/*.pkl
**/*.model
**/*.bin

# Experiment data (large files)
experiments/*/examples/
experiments/*/examples_*.json

# Log files
logs/
*.log

# Database files
data/db/
*.db
*.sqlite

# Python artifacts
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# Virtual environments
venv/
env/
ENV/

# OS specific files
.DS_Store
Thumbs.db

# Large datasets
data/train/*
!data/train/.gitkeep
data/validation/*
!data/validation/.gitkeep
data/test/*
!data/test/.gitkeep
data/nejm/*
!data/nejm/.gitkeep

# Keep minimal examples for testing
!data/train/current_train.json
!data/validation/current_validation.json
